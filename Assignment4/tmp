int printi(int num);
int prints(char * c);
int readi(int *eP);

int gcd(int m, int n)
{
    if (n!=0)
    {
		int a, b;
		a = m % n;
		b = gcd(n, a);
	        return b;
    }
    else 
       return m;
}

int main()
{
	int a, b, c, p;
	prints("********GCD*********\n");
	prints("a :");
	a = readi(&p);
	prints("b :");
	b = readi(&p);	
	prints("gcd(");
	printi(a);
	prints(",");
	printi(b);
	prints(") : ");
	c = gcd(a, b);
	printi(c);
	prints("\n");	
	return 0;
}
